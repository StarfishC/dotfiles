	"django1":{
		"prefix": "djbe",
		"body": "{% block ${1:name} %}${2:content}{% endblock ${3:name} %}$0",
		"description": "block-end块",
	},
	"django2": {
		"prefix": "dje",
		"body": "{% extends ${1:name} %}$0",
		"description": "extends块"
	},
	"django3": {
		"prefix": "dj#",
		"body": "{# ${1:comment} #}$0",
		"description": "django备注"
	},
	"django4": {
		"prefix": "dj%",
		"body": "{% $1 %}$0",
		"description": "django-{% %}"
	},
	"django5": {
		"prefix": "dj{",
		"body": "{{ $1 }}$0",
		"description": "django-{{ }}"
	},
	"django6": {
		"prefix": "djb",
		"body": "{% block ${1:name} %}$0",
		"description": "block块"
	},
	"django7": {
		"prefix": "djed",
		"body": "{% endblock ${2:name} %}$0",
		"description": "endblock块"
	}
}